<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.EmployeeChangeMapper">
    <resultMap id="employeeChangeMap" type="com.example.springboot.entity.EmployeeChange">
        <id column="rid" property="id"/>
        <result column="employee_id" property="employeeId"/>
        <result column="employee_name" property="employeeName"/>
        <result column="change_reason" property="changeReason"/>
        <result column="change_date" property="changeDate"/>
        <result column="entry_date" property="entryDate"/>
        <result column="regular_date" property="regularDate"/>
        <result column="job_level" property="jobLevel"/>
        <result column="employment_status" property="employmentStatus"/>
        <result column="work_location" property="workLocation"/>
        <result column="department" property="department"/>
        <result column="resignation_date" property="resignationDate"/>
    </resultMap>
</mapper>